*{margin:0;padding:0;list-style:none;text-decoration:none;}
header{
	width:100%;
	height:28px;
	background:#f5f5f5;
}
#header{
	width:1171px;
	height:28px;
	margin:0 auto;	
}
.headerLeft{
	float:left;
	position:relative;
}
.nowaddress{
	width:95px;
	overflow:hidden;
	border-left:1px solid #f5f5f5;
	border-right:1px solid #f5f5f5;
}
.nowaddress span{
	height:28px;
	line-height:28px;
	font-size:12px;
	padding:0 20px;
}
.nowaddress b{
	float:left;
	width:15px;
	height:28px;
	background:url(../images/top-1.png) no-repeat center;
	background-size:80%;
}
.headerLeft:hover .nowaddress{
	background:#fff;
	border-left:1px solid #000;
	border-right:1px solid #000;
}
.headerLeft:hover .nowaddress span{
	color:#f10180;
}
.headerLeft:hover .nowaddress b{
	background:url(../images/top-2.png) no-repeat center;
	background-size:80%;
}
.alladdress{
	width:235px;
	border:1px solid #000;
	position:absolute;
	background:#fff;
	top:28px;
	display:none;
}
.alladdress h4{
	border-bottom:1px dashed #000;
	line-height:28px;
	padding:0 10px;
}
.alladdress li{
	line-height:20px;
}
.alladdress b{
	font-size:12px;
	padding:0 5px;
}
.alladdress span{
	font-size:12px;
	padding:2px 5px;
	cursor:pointer;
}
.alladdress span:hover{
	color:#f10180;
}
.white_box{
	width:95px;
	height:3px;
	position:absolute;
	top:27px;
	z-index:2;
	background:#fff;
	left:1px;
	display:none;
}
.headerLeft:hover .alladdress,.headerLeft:hover .white_box{
	display:block;
}
.now{
	background:#f10180;
	color:#fff;
}
.alladdress .now:hover{
	color:#fff;
}
.headerRight{
	float:right;
	width:667px;
	height:28px;
}
.headerRight li{
	float:left;
	padding:0 14px;
	font-size:12px;
	line-height:28px;
	height:28px;
	position:relative;
	border-left:1px solid #f5f5f5;
	border-right:1px solid #f5f5f5;
	background:url(../images/wp-nav-cutline.png) no-repeat center right;
}
.headerRight .li_hover:hover{
	background:#fff;
	border-left:1px solid #ebebeb;
	border-right:1px solid #ebebeb;
}
.headerRight .last_li{
	background:#f5f5f5;
}
.headeraRight li a{
	color:#666;
}
.headerRight li a:hover{
	color:#f10180;
} 

.headerRight li b{
	float:right;
	height:28px;
	width:10px;
	background:url(../images/top-1.png) no-repeat center;
	background-size:100%;
}
.headerRight li:hover b{
	background:url(../images/top-2.png) no-repeat center;
	background-size:100%;
}
.login{
	width:270px;
}
.select_section{
	display:none;
	position:absolute;
	top:28px;
	left:-1px;
	background:#fff;
	border:1px solid #ebebeb;
}
.login .login_top{
	margin:10px 20px;
	overflow:hidden;
}
.login .user{
	float:left;
	width:40px;
	height:40px;
	background:url(../images/avatar-default.jpg) no-repeat center;
	background-size:100%;
	border-radius:50%;
}
.login_top span{
	border-bottom:1px dashed #666;
	float:right;
	width:165px;
	padding-left:5px;
}
.login_bottom{
	overflow:hidden;
	padding-left:20px;
	padding-left:10px;
}
.login_bottom span{
	float:left;
	width:80px;
	line-height:20px;
	margin-right:40px;
}
.login_bottom span a{
	font-size:12px;
}
.smallbox{
	width:100%;
	height:3px;
	left:0;
	position:absolute;
	top:27px;
	background:#fff;
	z-index:99;
}
.mycollect span a{
	text-align:center;
	display:block;
	width:140px;
}
.huiyuan span a{
	text-align:center;
	display:block;
	width:98px;
}
.kefu span a{
	text-align:center;
	display:block;
	width:86px;
}
.li_hover i{
	height:28px;
	width:12px;
	float:left;
	background:url(../images/header.png) no-repeat -100px -143px;
}
.li_hover:hover i{
	background:url(../images/header.png) no-repeat -125px -143px;
}
.myphone{
	padding:10px;
	text-align:center;
	left:-112px;
}
.myphone img{
	width:167px;
}
.gengduo{
	padding:10px;	
	left:-199px;	
}
.gengduo span{
	font-size:12px;
}
.shang{
	border-bottom:1px dashed #666;
	width:235px;
	padding-left:5px;
}
.xia{
	width:235px;
	padding-left:5px;
	padding-top:10px;
}



#logo{
	height:110px;
	width:1171px;
	margin:0 auto;
}
#logo img{
	height:97px;
}
.logo_a img{
	height:100px;
}
.promise_a{
	float:right;
	margin-right:30px;
}




.my_shoppingbag{
	float:right;
	width:122px;
	height:45px;
}
.my_shoppingbag>a{
	display:block;
	width:105px;
	height:12px;
	padding-left:17px;
	border-left:1px solid #dddddd;
	font-size:12px;
	margin:42px 10px 0 10px;
}
.my_shoppingbag>a span{
	position:relative;
	float:left;
	margin:-6px 10px 0 8px;
	width:15px;
	height:23px;
	background:url(../images/header.png) no-repeat 0 -58px;
	z-index:4;
}
.my_shoppingbag>a span b{
	position:absolute;
	width:17px;
	height:17px;
	top:-5px;
	right:-9px;
	background:#f10180;
	text-align:center;
	color:#fff;
	line-height:17px;
	z-index:5;
	border-radius:7px;
}
nav{
	width:100%;
	height:40px;
	background:#f10180;
}
#nav{
	width:1171px;
	height:40px;
	margin:0 auto;	
}
#nav a{
	padding:0 25px;
	line-height:40px;
	float:left;
	color:#fff;
	font-size:18px;
}
.now_html,#nav a:hover{
	background:#bd1067;
}
#nav .last_nav{
	float:right;
}
.gouwudai span a{
	text-align:center;
	display:block;
	width:86px;
}
.xiao{
	width:100px;
	height:3px;
	left:900px;
	position:absolute;
	top:50px;
	background:#000;
	z-index:99;
}
.my_shoppingbag a{
	height:50px;
	text-align:center;
	line-height:30px;
	position:absolute;
	top:10px;
}

.my_shoppingbag a:hover{

	background:#fff;
	border-top:1px solid #ebebeb;
	border-left:1px solid #ebebeb;
	border-right:1px solid #ebebeb;
}
#fixed_right{
	width:36px;
	position:fixed;
	top:0;
	right:0;
	z-index:999;
	background:#262626;
}
.myID{
	width:36px;
	height:42px;
	margin-top:110px;
	background:url(../images/sprite.png) no-repeat -82px 0;
}
.myID:hover{
	background:url(../images/sprite.png) no-repeat -82px -45px;
}
.myquan i,.mylogo i,.mythings i,.advice i,.totop i{
	display:block;
	width:36px;
	height:42px;
	margin-top:15px;
}
.myquan i{
	background:url(../images/sprite.png) no-repeat -164px 0 #262626;
}
.myquan:hover i{
	background:url(../images/sprite.png) no-repeat -41px 0;
}
.mylogo i{
	background:url(../images/sprite.png) no-repeat -41px -45px #262626;
}
.mylogo:hover i{
	background:url(../images/sprite.png) no-repeat -123px 0;
}
.mythings i{
	background:url(../images/sprite.png) no-repeat -123px -45px #262626;
}
.mythings:hover i{
	background:url(../images/sprite.png) no-repeat -164px -45px;
}
.left_bag{
	height:125px;
	width:36px;
	margin-top:10px;
	border-top:1px solid #4c4c4c;
	padding-bottom:5px;
	margin-top:15px;
}
.left_bag:hover{
	background:#df147f;
}
.left_bag b{
	display:block;
	width:36px;
	height:36px;
	background:url(../images/sprite.png) no-repeat -164px -130px;
}
.left_bag:hover b{
	background:url(../images/sprite.png) no-repeat -41px -130px;
}
.left_bag span{
	display:block;
	font-size:14px;
	padding:11px;
	color:#ccc;
}
.left_bag:hover span{
	color:#fff;
}
.left_bag div{
	width:19px;
	height:19px;
	border-radius:50%;
	background:#df147f;
	color:#FFF;
	text-align:center;
	line-height:19px;
	margin-left:8px;
}
.left_bag:hover div{
	background:#fff;
	color:#df147f;
}
.totop i{
	background:url(../images/sprite.png) no-repeat -205px 0 #262626;
}
.totop:hover i{
	background:url(../images/sprite.png) no-repeat 0 -130px;
}
.advice i{
	margin-top:10px;
	background:url(../images/sprite.png) no-repeat -82px -129px #262626;
}
.advice:hover i{
	background:url(../images/sprite.png) no-repeat -123px -129px;
}
.right_explain{
	position:relative;
	width:36px;
	height:42px;
	margin-top:15px;
}
.right_explain i{
	position:absolute;
	z-index:90;	
}

.right_explain p{
	border:1px solid #999999;
	width:117px;
	height:34px;
	text-align:center;
	line-height:34px;
	position:absolute;
	z-index:80;
	top:16px;
	background:#fff;
	border-right:0;
}