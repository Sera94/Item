*{
	margin:0;
	padding:0;
	text-decoration:none;
	list-style:none;
}
#big{
	width:100%;
	height:1452px;
	margin:0 auto;
}
#top{
	width:100%;
	height:1042px;
	background:#f6f6f6;
}
.box{
	width:960px;
	height:1012px;
	margin:0 auto;
	/* background:black; */
	padding-top:10px;
}
.header{
	width:100%;
	height:40px;
	background:linear-gradient(to top,#278cda,#1d6daa);
	border-radius:5px;
	overflow:hidden;
}
.header_left img{
	margin:6px 0 0 16px;
	float:left;
}
.header_mid{
	width:539px;
	height:100%;
	margin-left:110px;
	float:left;
}
.header_mid a{
	display:block;
	float:left;
	width:68px;
	height:100%;
	font-size:14px;
	color:#fff;
	border-right:1px solid #429bdf;
	padding-top:12px;
	padding-left:8px;
}
.header_right{
	width:146px;
	height:20px;
	margin-top:8px;
	border:2px solid #c6c7c8;
	border-radius:4px;
	float:left;
	overflow:hidden;
}
#input{
	width:119px;
	height:18px;
	float:left;
	cursor:pointer;
}
.header_right img{
	float:right;
	cursor:pointer;
}
.nav{
	width:100%;
	height:540px;
	background:url("../images/news-duba-ban.jpg") no-repeat;
	margin-top:20px;
}
.nav_top{
	width:100%;
	height:450px;
	overflow:hidden;
}
.nav_top_right{
	width:412px;
	height:314px;
	float:right;
	margin-top:63px;
}
.nav_top_rights{
	width:100%;
	height:76px;
	line-height:30px;
	margin-top:22px;
}
.nav_top_rights p{
	color:#fff;
	padding-left:50px;
	font-size:14px;
}
.nav_top_rights b{
	color:#F6FF16;
	font-size:22px;
}
.nav_top_rightz{
	width:100%;
	height:100px;
}
.nav_top_rightz input{
	width:260px;
	height:70px;
	color:#fff;
	font-size:28px;
	margin-left:50px;
	border:none;
	border-radius:4px;
	background:url("../images/ndb-btn-a.png") no-repeat 0 0;
}
.nav_top_rightz input:hover{
	background:url("../images/ndb-btn-a.png") no-repeat 0 -70px;
	cursor:pointer;
}
.nav_top_rightx{
	margin-left:46px;
	margin-top:-12px;
}
.nav_top_rightx a{
	font-size:12px;
	color:#2699FB;
	padding:0 8px;
}
.nav_top_rightx a:hover{
	text-decoration:underline;
}
.nav_bottom{
	width:100%;
	height:90px;
	overflow:hidden;
}
.nav_bottom img{
	float:left;
}
.nav_bottom p{
	float:left;
	color:#999999;
	font-size:14px;
	line-height:26px;
	padding-left:24px;
}
.nav_bottom1{
	width:230px;
	height:100%;
	float:left;
	margin:12px 0 0 30px;
}
.nav_bottom2{
	width:250px;
	height:100%;
	float:left;
	margin:14px 0 0 72px;
}
.nav_bottom3{
	width:244px;
	height:100%;
	float:left;
	margin:12px 0 0 60px;
}
.download{
	width:100%;
	height:150px;
	overflow:hidden;
	margin-top:20px;
}
.download1,.download2,.download3,.download4{
	width:226px;
	height:148px;
	border:1px solid #e3e3e3;
	background:#f0f0f0;
	float:left;
	background:url("../images/33_03.gif") repeat;
	margin-right:16px;
	overflow:hidden;
	position:relative;
}
.download h2{
	font-size:20px;
	margin:21px 0 0 10px;
	cursor:pointer;
}
.download p{
	font-size:12px;
	color:#6d6d6d;
	margin:12px 0 0 10px;
	line-height:20px;
}
.download input{
	width:84px;
	height:24px;
	background:#1e6fb0;
	border:none;
	margin:10px 0 0 10px;
	color:#fff;
	cursor:pointer;
}
.download a{
	font-size:12px;
	color:#6d6d6d;
}
.download a:hover{
	color:black;
}
.download1_left{
	width:138px;
	height:100%;
	float:left;
}
.download1_right{
	float:left;
	margin-top:40px;
	cursor:pointer;
}
.download2_left{
	width:150px;
	height:100%;
	float:left;
}
.download2_right .kis{
	float:left;
	margin-top:40px;
	position:absolute;
	left:140px;
	cursor:pointer;
}
.download2_right .hot{
	position:absolute;
	top:16px;left:142px;
}
.download3_left{
	width:160px;
	height:100%;
	float:left;
}
.download3_right{
	float:left;
	margin-top:40px;
	position:absolute;
	left:122px;
	cursor:pointer;
}
.download4_left{
	width:160px;
	height:100%;
	float:left;
}
.download4_right img{
	width:84px;
	height:84px;
}
.download4_right{
	float:left;
	margin-top:40px;
	position:absolute;
	left:132px;
	cursor:pointer;
}
.inner{
	width:958px;
	height:222px;
	overflow:hidden;
	margin-top:20px;
	border:1px solid #e7e7e7;
	background:#fff;
	overflow:hidden;
	position:relative;
}
.inner_left,.inner_mid,.inner_right{
	width:318px;
	height:180px;
	border-right:1px solid #dbdbdb;
	float:left;
	margin-top:20px;
	position:relative;
}
.inner_left1 h2{
	font-size:21px;
	margin:10px 0 0 20px;
	cursor:pointer;
}
.inner_left1 p{
	font-size:12px;
	color:#6d6d6d;
	margin:10px 0 0 20px;
	line-height:20px;
}
.inner_left1 input{
	width:84px;
	height:24px;
	background:#1e6fb0;
	border:none;
	margin:12px 0 0 20px;
	color:#fff;
	cursor:pointer;
}
.inner_left1{
	width:166px;
	height:100%;
	float:left;
}
.inner_left2{
	width:139px;
	height:100%;
	float:left;
	position:absolute;
	top:16px;left:156px;
	cursor:pointer;
}
.inner_left1 a{
	font-size:12px;
	color:#6d6d6d;
}
.inner_left1 a:hover{
	color:black;
}
.inner_mid_top{
	overflow:hidden;
	margin:6px 0 0 16px;
}
.inner_mid_top img{
	width:66px;
	height:63px;
	float:left;
}
.inner_mid_top span{
	display:block;
	width:214px;
	height:56px;
	float:left;
	margin-left:12px;
}
.inner_mid_top b{
	font-size:14px;
	color:#000;
	cursor:pointer;
}
.inner_mid_top p{
	font-size:12px;
	color:#6D6D6D;
	line-height:18px;
	margin-top:8px;
}
.inner_mid_bottom{
	width:300px;
	height:100px;
	position:relative;
}
.inner_mid_bottom ul{
	font-size:12px;
	line-height:20px;
	margin:2px 0 0 18px;
}
.red{
	color:#CC0000;
}
.red:hover{
	color:#000;
	cursor:pointer;
}
.black{
	color:#6D6D6D;
}
.black:hover{
	color:#000;
	cursor:pointer;
}
.inner_mid_bottom p{
	font-size:12px;
	color:#0066AA;
	position:absolute;
	top:96px; left:264px;
}
.inner_mid_bottom p:hover{
	color:#000;
	cursor:pointer;
}
.inner_right b{
	font-size:14px;
	margin:10px 0 0 20px;
}
.inner_right ul{
	margin:6px 0 0 22px;
}
.inner_right ul li{
	line-height:20px;
}
.inner_right ul li span{
	font-size:8px;
	color:#999999;
	cursor:pointer;
}
.inner_right ul li a{
	font-size:12px;
	color:#6d6d6d;
	cursor:pointer;
}
.inner_right ul li a:hover{
	color:#000;
}
#bottom{
	width:100%;
	height:322px;
	background:#f9f9f9;
}
.bottom_center{
	width:960px;
	height:100%;
	margin:0 auto;
	background:#fff;
	overflow:hidden;
}
.bottom_center11,.bottom_center12,.bottom_center13,.bottom_center14,.bottom_center15,.bottom_center16{
	width:158px;
	height:170px;
	border-right:1px solid #dbdbdb;
	float:left;
	margin-top:10px;
}
.bottom_center b{
	margin:20px 0 0 20px;
	font-size:12px;
	color:#6D6D6D;
}
.bottom_center ul li{
	font-size:12px;
	color:#6D6D6D;
	margin-top:10px;
	line-height:10px;
}
